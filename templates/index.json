    "collapsible_content_c6GLqX": {
      "type": "collapsible-content",
      "blocks": {
        "collapsible_row_EKFRUX": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Aloe vera",
            "icon": "check_box",
            "filled_icon": false,
            "custom_icon": "shopify://shop_images/50dc36ae-a8d2-43c6-9b5b-0f76e65f0970.webp",
